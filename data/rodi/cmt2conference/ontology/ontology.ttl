                                                                )
                                                  ] .



###  http://conference#has_contributions

:has_contributions rdf:type owl:ObjectProperty ;

                   rdfs:label "has contributions" ;

                   rdfs:range :Conference_contribution ;

                   rdfs:domain :Conference_volume .



###  http://conference#has_important_dates

:has_important_dates rdf:type owl:FunctionalProperty ,
                              owl:ObjectProperty ;

                     rdfs:label "has important dates" ;

