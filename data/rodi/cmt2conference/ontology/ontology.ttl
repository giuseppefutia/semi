                             rdfs:domain :Steering_committee ;

                             owl:inverseOf :has_a_steering_committee ;

                             rdfs:subPropertyOf :was_a_committee_of .



###  http://conference#was_a_track-workshop_chair_of

:was_a_track-workshop_chair_of rdf:type owl:InverseFunctionalProperty ,
                                        owl:ObjectProperty ;

                               rdfs:range :Conference_part ;

                               rdfs:domain :Track-workshop_chair .



###  http://conference#was_an_organizing_committee_of

:was_an_organizing_committee_of rdf:type owl:InverseFunctionalProperty ,
                                         owl:ObjectProperty ;

                                rdfs:range :Conference_volume ;

                                rdfs:domain :Organizing_committee ;

                                owl:inverseOf :has_an_organizing_committee ;

                                rdfs:subPropertyOf :was_a_committee_of .





