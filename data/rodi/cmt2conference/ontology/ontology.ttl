             rdfs:range :Active_conference_participant ;

             rdfs:domain :Presentation .



###  http://conference#is_part_of_conference_volumes

:is_part_of_conference_volumes rdf:type owl:ObjectProperty ;

                               rdfs:label "is part of" ;

                               rdfs:domain :Conference_part ;

                               rdfs:range :Conference_volume .



###  http://conference#is_submitted_at

:is_submitted_at rdf:type owl:ObjectProperty ;

                 rdfs:label "submitted at" ;

                 rdfs:domain :Conference_contribution ;

                 rdfs:range :Conference_volume ;

                 owl:inverseOf :has_contributions .



###  http://conference#is_the_1th_part_of

:is_the_1th_part_of rdf:type owl:InverseFunctionalProperty ,
                             owl:ObjectProperty ;

                    rdfs:label "is first part of" ;

                    rdfs:domain :Abstract ;

                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( :Paper
                                               :Poster
                                               :Presentation
                                             )
                               ] .



###  http://conference#issues

:issues rdf:type owl:InverseFunctionalProperty ,
                 owl:ObjectProperty ;

        rdfs:label "publishes" ;

        rdfs:range :Conference_proceedings ;

        rdfs:domain :Publisher ;

        owl:inverseOf :has_a_publisher .



###  http://conference#reviews

:reviews rdf:type owl:FunctionalProperty ,
