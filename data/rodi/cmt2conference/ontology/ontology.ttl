                            rdfs:range :Committee ;

                            rdfs:subPropertyOf :was_a_member_of .



###  http://conference#was_a_committee_chair_of

:was_a_committee_chair_of rdf:type owl:InverseFunctionalProperty ,
                                   owl:ObjectProperty ;

                          rdfs:domain :Chair ;

                          rdfs:range :Committee ;

                          owl:inverseOf :has_a_committee_chair ;

                          rdfs:subPropertyOf :was_a_member_of .



