###  http://conference#belong_to_a_conference_volume

:belong_to_a_conference_volume rdf:type owl:InverseFunctionalProperty ,
                                        owl:ObjectProperty ;

                               rdfs:label "date belongs to conference" ;

                               rdfs:range :Conference_volume ;

                               rdfs:domain :Important_dates .



###  http://conference#belongs_to_a_review_reference

:belongs_to_a_review_reference rdf:type owl:InverseFunctionalProperty ,
                                        owl:ObjectProperty ;

                               rdfs:label "topic of review preference" ;

                               rdfs:range :Review_preference ;

                               rdfs:domain :Topic .



###  http://conference#belongs_to_reviewers

:belongs_to_reviewers rdf:type owl:ObjectProperty ;

                      rdfs:label "belongs to reviewers" ;

                      rdfs:range :Reviewer ;

                      owl:inverseOf :has_a_review_reference_or_expertise ;

                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :Review_expertise
                                                  :Review_preference
                                                )
                                  ] .



###  http://conference#contributes

:contributes rdf:type owl:ObjectProperty ;

             rdfs:label "contributes" ;

             rdfs:range :Conference_document ;

             rdfs:domain :Person .



###  http://conference#gives_presentations

:gives_presentations rdf:type owl:ObjectProperty ;

                     rdfs:label "gives presentation" ;

                     rdfs:domain :Active_conference_participant ;

                     rdfs:range :Presentation ;

                     owl:inverseOf :is_given_by .



###  http://conference#has_a_committee_chair

:has_a_committee_chair rdf:type owl:FunctionalProperty ,
                                owl:ObjectProperty ;

                       rdfs:label "has committee chair" ;

                       rdfs:range :Chair ;

                       rdfs:domain :Committee ;

                       rdfs:subPropertyOf :has_members .



###  http://conference#has_a_committee_co-chair

:has_a_committee_co-chair rdf:type owl:ObjectProperty ;

                          rdfs:label "has committee co-chair" ;

                          rdfs:range :Co-chair ;

                          rdfs:domain :Committee ;

                          rdfs:subPropertyOf :has_members ;

                          owl:inverseOf :was_a_committe_co-chair_of .



###  http://conference#has_a_commtitee

:has_a_commtitee rdf:type owl:ObjectProperty ;

                 rdfs:label "has committee" ;

                 rdfs:range :Committee ;

                 rdfs:domain :Conference_volume .



###  http://conference#has_a_program_committee

:has_a_program_committee rdf:type owl:FunctionalProperty ,
                                  owl:ObjectProperty ;

                         rdfs:domain :Conference_volume ;

                         rdfs:range :Program_committee ;

                         rdfs:subPropertyOf :has_a_commtitee .



###  http://conference#has_a_publisher

:has_a_publisher rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;

                 rdfs:label "has publisher" ;

                 rdfs:domain :Conference_proceedings ;

                 rdfs:range :Publisher .



###  http://conference#has_a_review

:has_a_review rdf:type owl:InverseFunctionalProperty ,
                       owl:ObjectProperty ;

              rdfs:label "has review" ;

              rdfs:range :Review ;

              rdfs:domain :Reviewed_contribution ;

              owl:inverseOf :reviews .



###  http://conference#has_a_review_expertise

:has_a_review_expertise rdf:type owl:InverseFunctionalProperty ,
                                 owl:ObjectProperty ;

                        rdfs:label "reviewed with expertise level" ;

                        rdfs:range :Review_expertise ;

                        rdfs:domain :Submitted_contribution ;

                        owl:inverseOf :has_a_submitted_contribution .



###  http://conference#has_a_review_reference_or_expertise

:has_a_review_reference_or_expertise rdf:type owl:ObjectProperty ;

                                     rdfs:label "reviewer has reference or expertise" ;

                                     rdfs:domain :Reviewer ;

                                     rdfs:range [ rdf:type owl:Class ;
                                                  owl:unionOf ( :Review_expertise
                                                                :Review_preference
                                                              )
                                                ] .



###  http://conference#has_a_steering_committee

:has_a_steering_committee rdf:type owl:FunctionalProperty ,
                                   owl:ObjectProperty ;

                          rdfs:domain :Conference_volume ;

                          rdfs:range :Steering_committee ;

                          rdfs:subPropertyOf :has_a_commtitee .



###  http://conference#has_a_submitted_contribution

